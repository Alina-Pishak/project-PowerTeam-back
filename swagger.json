          {
            "in": "header",
            "name": "Authorization",
            "required": true,
            "type": "string",
            "description": "User`s token"
          }
        ],
        "responses": {
          "204": {
            "description": "Successful operation"
          },
          "401": {
            "description": "Missing header with authorization token"
          },
          "404": {
            "description": "Not Found"
